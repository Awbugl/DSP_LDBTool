<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Library that allows mods to add and edit Proto data. Also allows you to see Proto data, config ID\u0027s of mod Protos and change localized strings</Description>
    <PackageProjectUrl>https://dsp.thunderstore.io/package/xiaoye97/LDBTool/</PackageProjectUrl>
    <PackageId>DysonSphereProgram.Modding.LDBTool</PackageId>
    <Title>LDB Tool</Title>
	<PackageOutputDir>$(MSBuildThisFileDirectory)/Nuget/</PackageOutputDir>
	<PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
  </PropertyGroup>
</Project>
